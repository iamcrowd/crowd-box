        rdfs:isDefinedBy  : ;
        rdfs:label        "unità amministrativa di livello 1"@it , "administrative unit level 1"@en ;
        rdfs:range        rdfs:Literal ;
        owl:versionInfo   "stabile"@it , "stable" .

:addressID  a               owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:comment        "Proprietà utilizzata per rappresentare un identificatore univoco per ciascuna istanza di un indirizzo, Rappresenta un elemento cruciale delle specifiche INSPIRE. Sottoproprietà di identificativo."@it , "Property used to represent the globally unique identifier for each instance of an address. A crucial part of the INSPIRE data specifications. Subproperty of identifier."@en ;
