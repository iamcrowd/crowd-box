        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( :CulturalInstituteOrSite :NameInTime )
                            ] ;
        rdfs:isDefinedBy    : ;
        rdfs:label          "Nome Istituzionale"@it , "Institutional Name" ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  :name ;
        owl:versionInfo     "Stabile"@it , "Stable"@en .

:accessConditionFor  a    owl:ObjectProperty ;
        rdfs:comment      "Proprietà inversa di \"ha condizione di accesso\""@it , "Inverse property of \"has access condition\""@en ;
        rdfs:domain       :AccessCondition ;
        rdfs:isDefinedBy  : ;
        rdfs:label        "condizione di accesso per"@it , "access condition for"@en ;
        rdfs:range        [ a            owl:Class ;
                            owl:unionOf  ( :CulturalInstituteOrSite :Event )
                          ] ;
        owl:inverseOf     :hasAccessCondition .

:adminUnitL1  a           owl:DatatypeProperty ;
        rdfs:comment      "Property used to represent the uppermost administrative unit for the address of the site of the cultural institute or site, almost always a country."@en , "Proprietà utilizzata per l'unità amministrativa di più alto livello dell'indirizzo della sede di un istituto e luogo della cultura."@it ;
