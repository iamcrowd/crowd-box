        rdfs:domain       :NameInTime ;
        rdfs:isDefinedBy  : ;
        rdfs:label        "is valid during"@en , "è valido durante"@it ;
        rdfs:range        :TimeInterval ;
        owl:versionInfo   "stable" , "stabile"@it .

